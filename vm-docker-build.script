#!/bin/bash

docker stop web
docker stop srv
docker rm srv
docker rm web
docker build -t <NAMESPACE>/srv ./server
docker run -d --name srv -p <STATIC_IP>:<PORT>:<PORT from server/nodemon.json> <NAMESPACE>/srv
docker build -t <NAMESPACE>/web ./client
docker run -d --name web -p <STATIC_IP>:<OTHER_PORT>:<PORT from client/.env(.local)?> <NAMESPACE>/web